    "build:docs": "vite build",
    "lint": "eslint src/**/*.ts *.js --fix"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.4",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}

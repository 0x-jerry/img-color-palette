{
  "name": "mmcq.js",
  "version": "1.0.0",
  "main": "dist/core.js",
  "typings": "dist/core.d.ts",
  "repository": "https://github.com/0x-jerry/mmcq.js.git",
  "author": "Jerry Wang <x.jerry.wang@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "npm run wasm:dev && vite",
    "wasm": "wasm-pack build ./assembly-rust --target web",

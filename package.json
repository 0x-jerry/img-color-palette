{
  "name": "mmcq.js",
  "version": "1.2.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/0x-jerry/mmcq.js.git",
  "author": "Jerry Wang <x.jerry.wang@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "dev:wasm": "npm run wasm:dev && vite",
    "wasm": "wasm-pack build ./assembly-rust --target web",
    "wasm:dev": "wasm-pack build ./assembly-rust --target web --dev",
    "build": "npm run build:lib && cp assembly-rust/pkg/mmcq_bg.wasm dist/mmcq_bg.wasm",
    "build:lib": "tsup core/index.ts --dts --clean",
    "build:docs": "vite build",
    "release": "x-release"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@0x-jerry/x-release": "0.3.6",
    "@types/node": "17.0.45",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "@vitejs/plugin-react": "1.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsup": "5.12.9",
    "typescript": "4.7.4",
    "vite": "2.9.13"
  }
}